-    private static void visit(Resource root, Map<Resource, Boolean> seen) {
-        if (seen.containsKey(root)) {
-            return;
-        }
-        seen.put(root, Boolean.TRUE);
-        root.reachable = true;
-        if (root.references != null) {
-            for (Resource referenced : root.references) {
-                visit(referenced, seen);
-            }
-        }
-    }
