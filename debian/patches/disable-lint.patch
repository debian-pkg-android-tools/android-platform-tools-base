-        if (tagName.equals(TAG_ITEM)) {
-            String typeName = element.getAttribute(ATTR_TYPE);
-            if (!typeName.isEmpty()) {
-                return ResourceType.getEnum(typeName);
-            }
-        } else if ("string-array".equals(tagName) || "integer-array".equals(tagName)) {
-            return ResourceType.ARRAY;
-        } else {
