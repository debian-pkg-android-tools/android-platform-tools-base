-        } else if ("style".equals(tag)) {
-            int end = html.indexOf("</style>", offset + 1);
-            if (end != -1) {
-                // Attempt to tokenize the text as CSS
-                String css = html.substring(offset + 1, end);
-                tokenizeCss(from, css);
