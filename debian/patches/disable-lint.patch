-                } else {
-                    mExtractor.info("Deleting " + mFile);
-                }
-            }
-            if (!mDryRun) {
-                boolean deleted = mFile.delete();
-                if (!deleted) {
-                    Extractor.warning("Could not delete " + mFile);
-                }
-            }
-        }
-    }
-}
